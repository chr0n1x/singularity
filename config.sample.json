{
    "cacheConfig": "path/to/cached/config/file",
    "log": {
        "console": {
            "level": "debug",
            "colorize": true
        }
    },
    "build": {
        "jenkins": {
            "protocol" : "http",
            "host" : "bejankins.foo.bar.com:8080",
            "auth": {
                "project_token": "GLOBAL_TOKEN_FALL_BACK",
                "user": "JENKINS_USER",
                "password": "JENKINS_USER_PASSWORD"
            },
            "projects": {
                "repo1": "invoked_for_all_changes_in_repo1",
                "repo2": {
                    "project_token": "repo2project_token",
                    "change": {
                        "project": "change_project_for_repo2"
                    },
                    "proposal": [
                        {
                            "project": "project1_for_repo2_propsals"
                        },
                        {
                            "project": "special_proposal_project_for_repo2",
                            "project_token": "project_token_for_THIS_project"
                        }
                    ]
                }
            }
        }
    },
    "vcs": {
        "github": {
            "ignore_statuses": true,
            "auth": {
                "username": "CI_USER_GITHUB_NAME",
                "token": "OAUTH_TOKEN_FOR_CI_USER",
                "type": "oauth"
            },
            "repos": [
                "awsome_org_one/repo_one",
                "awsome_org_two/repo_two"
            ]
        }
    }
}
